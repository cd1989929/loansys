<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="spd.speeder.spdsample.domain.mapper.PostMapper">
	<resultMap type="Post" id="postResultMap"
		extends="spd.speeder.spdsample.domain.mapper.AbstractEntityMapper.abstractEntityResultMap">
		<result property="title" column="title" />
		<result property="content" column="content" />
	</resultMap>
	<select id="findAll" resultMap="postResultMap">
		SELECT * FROM T_POST ORDER BY T_POST.creation_datetime DESC
	</select>
</mapper>